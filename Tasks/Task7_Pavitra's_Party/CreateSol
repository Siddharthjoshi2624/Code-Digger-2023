from queue import Queue

n,m=map(int,input().split())
a=[[]for _ in range(n)]
for _ in range(m):
    x,y=map(int,input().split())
    x-=1
    y-=1
    a[x]+=y,
    a[y]+=x,

q=Queue()
u=[0]*n
d=[1e9]*n
d[0]=0
q.put(0)
u[0]=1
c=0
while not q.empty():
    v=q.get()
    if d[v]<=2:
        c+=1
    for x in a[v]:
        if not u[x]:
            u[x]=1
            q.put(x)
            d[x]=d[v]+1

print(c)
for i in range(1,n):
    if d[i]<=2:
        print(i+1,end=' ')
print()
